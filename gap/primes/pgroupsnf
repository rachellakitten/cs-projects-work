f := [[0,2], [1,2], [1,3], [2,3]];
pgroupsnf := function(f);

Sort(f, function(a, b)
  return a[1] < b[1];
end);

for i in [1..4] do
    addi := x -> x + Maximum(0, f[i][2] - f[i][1]);
    
    if i < Length(f) then
        for j in [i+1..4] do
            f[j][1] := addi(f[j][1]);  # Apply addi to f[j][1]
        od;
    fi;
od;

for i in [1..4] do
    Print(Minimum(f[i][1], f[i][2]), " ");
od;
end;
pgroupsnf(f);